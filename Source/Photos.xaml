<Window x:Class="WPfApplication.Photo_window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPfApplication"
        mc:Ignorable="d"
        Title="Просмотр документа" Height="376" Width="749" Loaded="Window_Loaded" Icon="/WPFIcon.ico">

    <Grid Margin="5,12,5,5">
        <TextBox x:Name="tb" TextWrapping="Wrap" BorderBrush="#FFA8A8A8" SelectionBrush="#FF2C49FF" VerticalScrollBarVisibility="Auto" Margin="10,10,10,10"  Background="White" />
        <Image x:Name="image_place" Margin="10,10,10,10" IsEnabled="False"/>
    </Grid>
</Window>
